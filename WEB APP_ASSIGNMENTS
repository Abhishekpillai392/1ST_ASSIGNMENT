34 Create a JSON data which contains book information for three books. Access all the details of every book like bookid,booktitle,bookprice.


=>


//ass34.html
<html>
<body>
    <hr>
    <h2 id="i1"></h2><br>
    <h2 id="i2"></h2><br>
    <h2 id="i3"></h2><br>
    <hr>
    <h2 id="i4"></h2><br>
    <h2 id="i5"></h2><br>
    <h2 id="i6"></h2><br>
    <hr>
    <h2 id="i7"></h2><br>
    <h2 id="i8"></h2><br>
    <h2 id="i9"></h2><br>
    <hr>
    
<script>
    var jso = {"books": [{"bookid":102,"booktitle":"Fault in our stars","bookprice":1200},{"bookid":133,"booktitle":"Harry potter : Goblet of Fire","bookprice":1850},{"bookid":252,"booktitle":"Revolution 2020","bookprice":1405}]}

    document.getElementById("i1").innerHTML=jso.books[0].bookid;
    document.getElementById("i2").innerHTML=jso.books[0].booktitle;
    document.getElementById("i3").innerHTML=jso.books[0].bookprice;
    
    document.getElementById("i4").innerHTML=jso.books[1].bookid;
    document.getElementById("i5").innerHTML=jso.books[1].booktitle;
    document.getElementById("i6").innerHTML=jso.books[1].bookprice;
    
    document.getElementById("i7").innerHTML=jso.books[2].bookid;
    document.getElementById("i8").innerHTML=jso.books[2].booktitle;
    document.getElementById("i9").innerHTML=jso.books[2].bookprice;
        
    /* document.write(jso.books[0].bookid);
          document.write(jso.books[0].booktitle);
          document.write(jso.books[0].bookprice);*/

</script>   
</body>
</html>

