33 Create a form to accept password and username .3 text boxes for Username, Password and Retype Password. If Both passwords are matched then only form should be 
   submitted and display the username in the servlet.


=>


//ass33.html

<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Insert title here</title>
</head>
<body>
<form action="ass_ser33">
USERNAME <input type="text" name="nm">
PASSWORD <input type="password" name="pass">
RETYPE-PASSWORD <input type="password" name="pas">
<input type="submit" value="Go">
</form>
</body>
</html>

//servlet - 33

package PROJECTS;

import java.io.IOException;
import java.io.PrintWriter;

import javax.servlet.ServletException;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

public class ass_ser33 extends HttpServlet {
	private static final long serialVersionUID = 1L;
    
	protected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
	PrintWriter pw = response.getWriter();
	
	String us = request.getParameter("nm");
	
	String pa = request.getParameter("pass");
	
	String p  = request.getParameter("pas");
	
	if(us.equals("abhi123"))
	{
		if(pa.contentEquals("passabhi"))
		{
			if(pa.contentEquals(p))
			{
				pw.print("WELCOME "+us);
			}
			else
			{
				pw.print("PASSWORD DOESNT MATCH,PLEASE RETYPE IT");
			}
		}
		else
		{
			pw.print("PASSWORD IS INCORRECT");
		}	
	}
	else
	{
		pw.print("USERNAME IS INVALID");
	}	
}
	protected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		doGet(request, response);
	}

}


