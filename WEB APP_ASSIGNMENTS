<html>
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<style>
    img
    {
       border-style: solid;
       border-width: 5px;
       border-color: red;  
    }
</style>
</head>
<body>
    <img src="px.jpeg" style="width: 150px;height: 150px">
    
    <script>
    $(document).ready(function(){
        $("button#b1").click(function(){
            $("img").animate({
                width:"+=100px",
                height:"+=100px",
                borderStyle:"solid",
                borderWidth:"15px",
                borderColor:"red",
            });
        });
    })
    
    </script>
    
    <button id="b1">CLICK HERE</button>
</body>
</html>
