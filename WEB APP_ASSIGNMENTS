31 Make a form in Javascript where 2 text boxes accept numbers. Create 4 buttons ADD,SUB,MUL,DIV. When clicked on a button display the Answer in Heading tag.


=>


//ass31.html

<html>
<head>
    
</head>
<body>
    <button onclick="func()">CLICK HERE</button>

    <h1 id="i1"> </h1>
    <h1 id="i2"> </h1>
    
    <script>
    temp=true;
    function func()
        {
            if(temp)
            {
                var entry = "<form name='myform'><input type=number name='num1'><input type=number name='num2'><input type=button value='SUM' onclick=sum()><input type=button value='SUB' onclick=sub()><input type=button value='MUL' onclick=mul()><input type=button value='DIV' onclick=div()></form>";    
                
                document.getElementById("i1").innerHTML=entry;
                temp= false;      
            }
            else
            {
                document.getElementById("i1").innerHTML="";
                temp= true;
            } 
        }
        
        function sum()
        {
            var n1 = document.myform.num1.value;
            var n2 = document.myform.num2.value;
            
            var add =Number(n1) + Number(n2);
            
            document.getElementById("i2").innerHTML=add;
        }
        
        function sub()
        {
            var n1 = document.myform.num1.value;
            var n2 = document.myform.num2.value;
            
            var subs = n1 - n2;
            
            document.getElementById("i2").innerHTML=subs;
        }
        
        function mul()
        {
            var n1 = document.myform.num1.value;
            var n2 = document.myform.num2.value;
            
            var prod = n1 * n2;
            
            document.getElementById("i2").innerHTML=prod;
        }
        
        function div()
        {
            var n1 = document.myform.num1.value;
            var n2 = document.myform.num2.value;
            
            var by = n1 / n2;
            
            document.getElementById("i2").innerHTML=by;
        }
    </script>
</body>
</html>

