<script>

    var student = {name : "AVINASH",
                   skills :["Efficient","Drawing","Planner","Workholic","Patiency"] 
                  };
    //document.write(student.name+" IS"+"<br>");
    
    //document.write(student.skills[0]);
    
    var jsonstud = JSON.stringify(student);
    
    
 document.getElementById("i1").innerHTML=jsonstud;
    
</script>
</body>
</html>
   

