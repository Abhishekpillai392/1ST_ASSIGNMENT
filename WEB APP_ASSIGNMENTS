25 Make an integer array of 5 numbers in a function and print the average of array.

=>

//ass25.html

<html>
<body>
<script>
    function fun()
    {
      var ar = [10,12,14,16,18];
      
      var total=0; 
      document.write("LENGTH OF THE ARRAY : "+ar.length);
        
      /*document.write("AVERAGE WILL BE : "+(ar[0]+ar[1]+ar[2]+a[3]+a[4])/ar.length);*/
        
        for(var i=0;i < ar.length;i++)
            {
                total+= ar[i];
            }
        
        var avg= total / ar.length;
        
        console.log("AVERAGE OF THE ARRAY WILL BE : "+avg);
        
        document.write("<br>"+"AVERAGE OF THE ARRAY WILL BE : "+avg);
    }
    
    fun();
    
</script>    
    
</body>
</html>

