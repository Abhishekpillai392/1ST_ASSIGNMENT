    <script>
     var temp=true;
    
    function fun()
        { 
            
            var entry="<form>USERNAME<input type=text>PASSWORD<input type=text></form>";

            if(temp)
                {
                    document.getElementById("i1").innerHTML=entry;
                    temp=false;
