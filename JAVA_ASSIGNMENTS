	public String alpha;
    public Alphabetsa(String a)
    {
   	 if(a.length()==1)
   	 {
    	alpha = a;
   	 }
    }
    public void print()
    {
    	System.out.println("ALPHABET IS : "+alpha.toUpperCase());
    }
	public static void main(String[] args) {	
	}

}

package basicprograms5;

public class Access  {

	public static void main(String[] args) {
		
		Alphabetsa ab1 = new Alphabetsa("a");
		Alphabetsa ab2 = new Alphabetsa("t");
