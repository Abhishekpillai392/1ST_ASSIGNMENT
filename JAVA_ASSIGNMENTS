Q.5 Write a program to enter a year and check if the year is a leap year or not.
=

package BasicPrograms;

import java.util.Scanner;

public class ASSIGN5 {
	int yr;
	
	public void lpyear(int ab)
	{
		if(ab%4==0)
		{
			System.out.println(ab+" IS A LEAP YEAR ");
		}
		else
		{
			System.out.println(ab+" IS NOT A LEAP YEAR ");
		}
	}
	public static void main(String[] args) {
		
		ASSIGN5 val= new ASSIGN5();
		
		Scanner set = new Scanner(System.in);
		
		val.lpyear(2000);
		
		System.out.println();
		
		System.out.println("ENETR THE YEAR : ");
		
		int year = set.nextInt();
		
		val.lpyear(year);
	    set.close();
	}
	

}

