Q.2 Write a program to Define employee Company as static and display the details of employee like eid,ename,salary with company name for 5 employees.
=

package BasicPrograms;

public class ASSIGN2 {
	 int eid,salary;
	 String ename;
	 static String companynm;

	 public static void getcompanynm()
	    {
	    	companynm = "GO.DIGIT";
	    }
	 
	 public void getothdetails(int id,String nm)
	 {
		 eid = id;
		 ename = nm;
		 
		 System.out.println("EID : "+eid);
		 System.out.println("NAME : "+ename);
		 
	 }
	 
	 public void showothdetails(int sal)
	 {
		 salary = sal;
		 
		 System.out.println("SALARY : "+salary);
		 System.out.println("COMPANY NAME : "+companynm);	 
	 }

	public static void main(String[] args) {
    
		ASSIGN2 emp1 = new ASSIGN2();
		ASSIGN2 emp2 = new ASSIGN2();
		ASSIGN2 emp3 = new ASSIGN2();
		ASSIGN2 emp4 = new ASSIGN2();
		ASSIGN2 emp5 = new ASSIGN2();
		
		getcompanynm();
		
		emp1.getothdetails(101,"ABHIJEETH");
		emp1.showothdetails(20000);
		System.out.println("================================================");

		emp2.getothdetails(102,"SIMRAN");
		emp2.showothdetails(30000);
		System.out.println("================================================");

		emp3.getothdetails(103,"DHANUSH");
		emp3.showothdetails(20000);
		System.out.println("================================================");
		
		emp4.getothdetails(104,"AMIT");
		emp4.showothdetails(25000);
		System.out.println("================================================");

		emp5.getothdetails(105,"FAHAAD");
		emp5.showothdetails(23000);
		System.out.println("================================================");
     }
}
	
